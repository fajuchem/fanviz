{
  "manifest_version": 3,
  "name": "React Content Script",
  "version": "1.0.0",
  "action": {
    "default_title": "Popup",
    "default_popup": "index.html",
    "default_icon": "logo192.png"
  },
  "icons": {},
  "content_scripts": [
    {
      "matches": [
        "https://github.com/fajuchem/poc-codecov/pull/*/files",
        "https://github.com/fajuchem/automatic-waffle/pull/*/files"
      ],
      "js": ["src/main.tsx"],
      "media": []
    }
  ]
}
